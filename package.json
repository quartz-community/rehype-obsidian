{
  "name": "@quartz-community/rehype-obsidian",
  "version": "0.1.0",
  "description": "Rehype plugin for Obsidian-flavored HTML transforms (block references, YouTube embeds, checkboxes, mermaid).",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/hast": "^3.0.4",
    "@types/node": "^22.0.0",
    "rehype-parse": "^9.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "unified": "^11.0.5",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=22"
  }
